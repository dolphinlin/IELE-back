<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-primary">
      <div class="panel-heading">
        期刊列表
      </div>
      <!-- /.panel-heading -->
      <div class="panel-body">
        <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
          <thead>
            <tr>
              <th>#</th>
              <th>期刊名稱</th>
              <th>操作</th>
            </tr>
          </thead>
          <tbody>
            {{#each journals}}
              <tr>
                <td>#</td>
                <td><a href="/{{procJournal path}}">{{title}}</a></td>
                <td>
                  <a href="/admin/journal/{{id}}/edit" class="btn btn-primary btn-xs">編輯</a>
                </td>
              </tr>
            {{/each}}
          </tbody>
        </table>
        <!-- /.table-responsive -->
      </div>
      <!-- /.panel-body -->
    </div>
    <!-- /.panel -->
  </div>
  <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
